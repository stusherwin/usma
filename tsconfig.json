{
    "compilerOptions": {
        "outDir": "./client/static/js/",
        "sourceMap": true,
        "noImplicitAny": true,
        "module": "commonjs",
        "target": "es5",  
        "lib": [ "es2015", "dom" ], 
        "jsx": "react",
        "strictNullChecks": true,
        "baseUrl": "./client/src",
        "paths": {
          "admin/*": [ "admin/*" ],
          "household/*": [ "household/*" ],
          "order/*": [ "order/*" ],
          "product/*": [ "product/*" ],
          "util/*": [ "util/*" ],
        },
        "esModuleInterop": true
    },
    "include": [
        "./client/src/**/*"
    ]
}